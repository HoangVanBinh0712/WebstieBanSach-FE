\"\",\r\n    lastname: \"\",\r\n    phone: \"\",\r\n    address: \"\",\r\n  });\r\n  const [error, setError] = useState([]);\r\n\r\n  useEffect(() => {\r\n    let isMounted = true;\r\n\r\n    axios.get(`/api/user/cart`).then((res) => {\r\n      if (isMounted) {\r\n        if (res.status === 200) {\r\n          setCart(res.data.data);\r\n          setLoading(false);\r\n        } else if (res.status === 401) {\r\n          history.push(\"/\");\r\n          swal(\"Warning\", res.data.message, \"error\");\r\n        }\r\n      }\r\n    });\r\n\r\n    return () => {\r\n      isMounted = false;\r\n    };\r\n  }, [history]);\r\n\r\n  const handleInput = (e) => {\r\n    e.persist();\r\n    setCheckoutInput({ ...checkoutInput, [e.target.name]: e.target.value });\r\n  };\r\n\r\n  const submitOrder = (e, payment) => {\r\n    e.preventDefault();\r\n\r\n    var data = {\r\n      name: checkoutInput.firstname + \" \" + checkoutInput.lastname,\r\n      phone: checkoutInput.phone,\r\n      address: checkoutInput.address,\r\n      payment: payment,\r\n    };\r\n    axios.post(\"api/user/order\", data).then((res) => {\r\n      if (res.status === 200) {\r\n        swal(\"Success\", \"Order successfully !\", \"success\");\r\n        //Send to orders page\r\n        history.push(\"/orders\");\r\n\r\n      }\r\n    });\r\n  };\r\n\r\n  if (loading) {\r\n    return <h4>Loading Checkout...</h4>;\r\n  }\r\n\r\n  var checkout_HTML = \"\";\r\n  if (cart.length > 0) {\r\n    checkout_HTML = (\r\n      <div>\r\n        <div className=\"row\">\r\n          <div className=\"col-md-7\">\r\n            <div className=\"card\">\r\n              <div className=\"card-header\">\r\n                <h4>Basic Information</h4>\r\n              </div>\r\n              <div className=\"card-body\">\r\n                <div className=\"row\">\r\n                  <div className=\"col-md-6\">\r\n                    <div className=\"form-group mb-3\">\r\n                      <label> First Name</label>\r\n                      <input\r\n                        type=\"text\"\r\n                        name=\"firstname\"\r\n                        onChange={handleInput}\r\n                        value={checkoutInput.firstname}\r\n                        className=\"form-control\"\r\n                      />\r\n                      <small className=\"text-danger\">{error.firstname}</small>\r\n                    </div>\r\n                  </div>\r\n                  <div className=\"col-md-6\">\r\n                    <div className=\"form-group mb-3\">\r\n                      <label> Last Name</label>\r\n                      <input\r\n                        type=\"text\"\r\n                        name=\"lastname\"\r\n                        onChange={handleInput}\r\n                        value={checkoutInput.lastname}\r\n                        className=\"form-control\"\r\n                      />\r\n                      <small className=\"text-danger\">{error.lastname}</small>\r\n                    </div>\r\n                  </div>\r\n                  <div className=\"col-md-6\">\r\n                    <div className=\"form-group mb-3\">\r\n                      <label> Phone Number</label>\r\n                      <input\r\n                        type=\"number\"\r\n                        name=\"phone\"\r\n                        onChange={handleInput}\r\n                        value={checkoutInput.phone}\r\n                        className=\"form-control\"\r\n                      />\r\n                      <small className=\"text-danger\">{error.phone}</small>\r\n                    </div>\r\n                  </div>\r\n                  <div className=\"col-md-12\">\r\n                    <div className=\"form-group mb-3\">\r\n                      <label> Full Address</label>\r\n                      <textarea\r\n                        rows=\"3\"\r\n                        name=\"address\"\r\n                        onChange={handleInput}\r\n                        value={checkoutInput.address}\r\n                        className=\"form-control\"\r\n                      ></textarea>\r\n                      <small className=\"text-danger\">{error.address}</small>\r\n                    </div>\r\n                  </div>\r\n\r\n                  <div className=\"col-md-12\">\r\n                    <div className=\"form-group text-end\">\r\n                      <button\r\n                        type=\"button\"\r\n                        className=\"btn btn-primary mx-1\"\r\n                        onClick={(e) => submitOrder(e, 1)}\r\n                      >\r\n                        Place Order\r\n                      </button>\r\n\r\n                      <button\r\n                        type=\"button\"\r\n                        className=\"btn btn-warning mx-1\"\r\n                        onClick={(e) => submitOrder(e, 2)}\r\n                      >\r\n                        Pay Online\r\n                      </button>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div className=\"col-md-5\">\r\n            <table className=\"table table-bordered\">\r\n              <thead>\r\n                <tr>\r\n                  <th width=\"50%\">Book</th>\r\n                  <th>Price</th>\r\n                  <th>Qty</th>\r\n                  <th>Total</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                {cart.map((item, idx) => {\r\n                  totalCartPrice += item.book.price * item.quantity;\r\n                  return (\r\n                    <tr key={idx}>\r\n                      <td>{item.book.name}</td>\r\n                      <td>{item.book.price}</td>\r\n                      <td>{item.quantity}</td>\r\n                      <td>{item.book.price * item.quantity}</td>\r\n                    </tr>\r\n                  );\r\n  
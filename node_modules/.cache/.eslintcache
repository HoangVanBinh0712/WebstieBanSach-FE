n  };\r\n  function updateCartQuantity(bookId, quantity) {\r\n    axios\r\n      .put(`/api/user/cart`, {\r\n        bookId: bookId,\r\n        quantity: quantity,\r\n      })\r\n      .then((res) => {\r\n        if (res.data.status === 200) {\r\n          swal(\"Success\", res.data.message, \"success\");\r\n        }\r\n      });\r\n  }\r\n\r\n  const deleteCartItem = (e, bookId) => {\r\n    e.preventDefault();\r\n\r\n    const thisClicked = e.currentTarget;\r\n    thisClicked.innerText = \"Removing\";\r\n\r\n    axios.delete(`/api/user/cart/${bookId}`).then((res) => {\r\n      if (res.status === 200) {\r\n        swal(\"Success\", res.data.message, \"success\");\r\n        thisClicked.closest(\"tr\").remove();\r\n        let carts = cart.filter((x) => x.book.id !== bookId);\r\n        if (res.data.success) {\r\n          setCart([]);\r\n          setCart(carts);\r\n        }\r\n      } else {\r\n        swal(\"Error\", res.data.message, \"error\");\r\n        thisClicked.innerText = \"Remove\";\r\n      }\r\n    });\r\n  };\r\n\r\n  if (loading) {\r\n    return <h4>Loading Product Detail...</h4>;\r\n  }\r\n\r\n  var cart_HTML = \"\";\r\n  if (cart.length > 0) {\r\n    cart_HTML = (\r\n      <div>\r\n        <div className=\"table-responsive\">\r\n          <table className=\"table table-bordered\">\r\n            <thead>\r\n              <tr>\r\n                <th>Image</th>\r\n                <th>Product</th>\r\n                <th >Price</th>\r\n                <th >Quantity</th>\r\n                <th >Total Price</th>\r\n                <th>Remove</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              {cart.map((item, idx) => {\r\n                totalCartPrice += item.book.price * item.quantity;\r\n                return (\r\n                  <tr key={idx}>\r\n                    <td width=\"10%\">\r\n                      <img\r\n                        src={`https://image.freepik.com/free-photo/books_87394-845.jpg`}\r\n                        alt={item.book.name}\r\n                        width=\"50px\"\r\n                        height=\"50px\"\r\n                      />\r\n                    </td>\r\n                    <td>{item.book.name}</td>\r\n                    <td width=\"15%\" >\r\n                      {item.book.price}\r\n                    </td>\r\n                    <td width=\"15%\">\r\n                      <div className=\"input-group\">\r\n                        <button\r\n                          type=\"button\"\r\n                          onClick={() => handleDecrement(item.book.id)}\r\n                          className=\"input-group-text\"\r\n                        >\r\n                          -\r\n                        </button>\r\n                        <div className=\"form-control text-center\">\r\n                          {item.quantity}\r\n                        </div>\r\n                        <button\r\n                          type=\"button\"\r\n                          onClick={() => handleIncrement(item.book.id)}\r\n                          className=\"input-group-text\"\r\n                        >\r\n                          +\r\n                        </button>\r\n                      </div>\r\n                    </td>\r\n                    <td width=\"15%\" >\r\n                      {item.book.price * item.quantity}\r\n                    </td>\r\n                    <td width=\"10%\">\r\n                      <button\r\n                        type=\"button\"\r\n                        onClick={(e) => deleteCartItem(e, item.book.id)}\r\n                        className=\"btn btn-danger btn-sm\"\r\n                      >\r\n                        Remove\r\n                      </button>\r\n                    </td>\r\n                  </tr>\r\n                );\r\n              })}\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n        <div className=\"row\">\r\n          <div className=\"c